- blog_hash = Blog.blog_groups
- title = "Blog Archive"
= rounded_corner(:size => :small, :title => title) do
  %table.widget_small.greetings
    %tbody
      - blog_hash.each do |b|
        %tr
          %td= link_to "#{b.month} #{b.year}", blog_archive_profile_blog_path(@p,b.month,b.year)
          %td= "(#{b.cnt})"